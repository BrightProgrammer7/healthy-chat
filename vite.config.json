{
  "root": "./",
  "build": {
    "outDir": "./dist"
  },
  "server": {
    "port": 3000,
    "open": true,
    "proxy": {
      "/api": {
        "target": "http://localhost:4000",
        "changeOrigin": true,
        "rewrite": "(path) => path.replace(/^/api/, '')"
      }
    }
  },
  "alias": {
    "nano": "./app/"
  }
}
